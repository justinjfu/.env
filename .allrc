function pprofile {
    python -m cProfile -o .profile.txt $1
    #python -c 'import pstats; p=pstats.Stats(".profile.txt"); p.sort_stats("cumulative").print_stats(30)'
    python -c 'import pstats; p=pstats.Stats(".profile.txt"); p.sort_stats("time").print_stats(30)'
    rm .profile.txt
}
